<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_tree_costrate" model="ir.ui.view">
        <field name="name">cost.rate.tree</field>
        <field name="model">cost.rate</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="job_type" options="{'no_open': True, 'no_create': True}"/>
                <field name="description" />
                <field name="create_date" string="Creation Date"/>
            </tree>
        </field>
    </record>
</odoo>