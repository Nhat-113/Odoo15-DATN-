<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_tree_costrate" model="ir.ui.view">
        <field name="name">cost.rate.tree</field>
        <field name="model">cost.rate</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <!-- <field name="role"/> -->
                <field name="job_type" readonly="1"/>
                <field name="description" />
                <field name="currency_usd_id" invisible="1" />
                <field name="currency_yen_id" invisible="1" />
                <field name="currency_vnd_id" invisible="1" />
                <field name="currency_sgd_id" invisible="1" />
                <field name="cost_usd" />
                <field name="cost_yen" />
                <field name="cost_vnd" />
                <field name="cost_sgd" />
                <field name="create_date" string="Creation Date"/>
            </tree>
        </field>
    </record>
</odoo>